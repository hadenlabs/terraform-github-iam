variable "team" {
  description = "This team to create"
  type = object({
    name        = string
    description = string
    privacy     = string
  })
}

variable "maintainers" {
  description = "This maintainers to create"
  type        = list(string)
  default = []
}

variable "members" {
  description = "This members to create"
  type        = list(string)
  default = []
}

variable "github" {
  description = "vars for github"
  type = object({
    token        = string
    organization = string
  })
}
